package com.ahmed.oop;

class A {
	void m1(int[] arr) {
		for (int x : arr) {
			System.out.println(x);
		}

	}

	void m2(int p,int... args) {
		System.out.println();
		for (int i = 0; i <= 4; i++) {
			System.out.println(args[i]);
		}

	}
}

public class TOAcceptRandom {
	public static void main(String[] args) {
		int[] arr = new int[] { 10, 20, 30, 40, 50 };
		A a = new A();
		a.m1(arr);
		a.m2(100, 200, 300, 400, 500);

	}

}
