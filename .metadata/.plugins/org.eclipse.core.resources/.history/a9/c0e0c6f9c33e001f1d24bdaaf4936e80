package com.ahmed.thread;

public class YieldMethod {
	public static void main(String[] args) {
		Runnable task1 = () -> {
			for (int i = 1; i <= 10; i++) {
				System.out.println("i = " + i);
				Thread.yield();
			}
		};
		
		Runnable task2=()->{
			for(int j=11;j<=20;j++) {
				sysouwrite a program to join two threads using lambda expression in java
			}
		}

	}

}
