package com.ahmed.oop;
class Parent {
	protected int x;

	Parentn() {
		x = 10;
	}

}

class Child extends Parent{
	private int y;

	Child() {
		super();
		y = 20;
	}

	void display() {
		System.out.println(x);
		System.out.println(y);
	}

}
public class Main {
	public static void main(String[] args) {
		
	}

}
