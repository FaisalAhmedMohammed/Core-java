package com.methods.co;

public class Factorial {
	public void fact(int n) {
		if (n < 0) {
			System.out.println("Invalid number");
		}
		int facto = 1;
		for (int i = n; i >= 1; i--) {
			facto = facto * i;

		}
		System.out.println(facto);
	}

	public static void main(String[] args) {
		Factorial fc = new Factorial();
		fc.fact();

	}

}
